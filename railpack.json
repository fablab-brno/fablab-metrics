{
	"provider": "node",
	"packages": {
		"node": "24",
		"python": "3.11",
		"poetry": "2.2.1"
	},
	"steps": {
		"python-install": {
			"commands": [
				{ "src": "pyproject.toml", "dest": "pyproject.toml" },
				{ "src": "poetry.lock", "dest": "poetry.lock" },
				"poetry config virtualenvs.in-project true",
				"poetry install",
				"ls -l",
				"ls dataloader -l"
			],
			"deployOutputs": [

			]
		},
		"install": {
			"inputs": [
				"...",
				{ "step": "python-install" }
			]
		}
	}
}