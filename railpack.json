{
	"provider": "node",
	"packages": {
		"node": "24",
		"python": "3.11",
		"poetry": "2.2.1"
	},
	"deploy": {
		"inputs": [
			"...",
			{
				"step": "packages:mise",
				"include": [
					"/mise/shims",
					"/mise/installs"
				]
			},
			{
				"step": "build",
				"include": ["."]
			}
		],
		"commands": [
			"apt-get update",
			"apt-get install curl",
			"mise uninstall poetry@2.2.1",
			"mise install poetry@2.2.1",
				"poetry config virtualenvs.in-project true",
				"poetry install",
			"..."
		]
	}
}